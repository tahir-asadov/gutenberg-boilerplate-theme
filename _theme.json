{
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#FFFFFF",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#2f2f2f",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#428bdb",
					"name": "Blue",
					"slug": "blue"
				},
				{
					"color": "#ffaa30",
					"name": "Orange",
					"slug": "orange"
				},
				{
					"color": "#f4f4f4",
					"name": "Light Gray",
					"slug": "light-gray"
				},
				{
					"color": "#b9d1ec",
					"name": "Light Blue",
					"slug": "light-blue"
				},
				{
					"color": "#ffecd1",
					"name": "Light Orange",
					"slug": "light-orange"
				},
				{
					"color": "#b3b3b3",
					"name": "Dark Gray",
					"slug": "dark-gray"
				},
				{
					"color": "#2267b3",
					"name": "Dark Blue",
					"slug": "dark-blue"
				},
				{
					"color": "#ee920f",
					"name": "Dark Orange",
					"slug": "dark-orange"
				},
				{
					"color": "color-mix(in srgb, currentColor 20%, transparent)",
					"name": "Accent 6",
					"slug": "accent-6"
				}
			]
		},
		"layout": {
			"contentSize": "1024px",
			"wideSize": "1440px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fontSizes": [
				{
					"fluid": false,
					"name": "X Small",
					"size": "0.875rem",
					"slug": "x-small"
				},
				{
					"fluid": false,
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.75rem",
						"min": "1.5rem"
					},
					"name": "Medium",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.875rem"
					},
					"name": "Large",
					"size": "1.875rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2.5rem"
					},
					"name": "Extra Large",
					"size": "2.5rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "3.75rem",
						"min": "3rem"
					},
					"name": "Extra Extra Large",
					"size": "3rem",
					"slug": "xx-large"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"name": "Manrope",
					"slug": "manrope",
					"fontFamily": "Manrope, sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/manrope/Manrope-VariableFont_wght.woff2"
							],
							"fontWeight": "200 800",
							"fontStyle": "normal",
							"fontFamily": "Manrope"
						}
					]
				},
				{
					"name": "Inter",
					"slug": "inter",
					"fontFamily": "Inter, sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/inter/Inter-VariableFont_opsz,wght.woff2"
							],
							"fontWeight": "100 800",
							"fontStyle": "normal",
							"fontFamily": "Inter"
						},
						{
							"src": [
								"file:./assets/fonts/inter/Inter-Italic-VariableFont_opsz,wght.woff2"
							],
							"fontWeight": "100 800",
							"fontStyle": "italic",
							"fontFamily": "Inter"
						}
					]
				},
				{
					"name": "Montserrat",
					"slug": "montserrat",
					"fontFamily": "Montserrat, sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/montserrat/Montserrat-VariableFont_wght.woff2"
							],
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontFamily": "Inter"
						},
						{
							"src": [
								"file:./assets/fonts/montserrat/Montserrat-Italic-VariableFont_wght.woff2"
							],
							"fontWeight": "100 900",
							"fontStyle": "italic",
							"fontFamily": "Inter"
						}
					]
				},
				{
					"name": "Open Sans",
					"slug": "open-sans",
					"fontFamily": "\"Open Sans\", sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/open-sans/OpenSans-VariableFont_wdth,wght.woff2"
							],
							"fontWeight": "300 800",
							"fontStyle": "normal",
							"fontFamily": "Inter"
						},
						{
							"src": [
								"file:./assets/fonts/open-sans/OpenSans-Italic-VariableFont_wdth,wght.woff2"
							],
							"fontWeight": "300 800",
							"fontStyle": "italic",
							"fontFamily": "Inter"
						}
					]
				},
				{
					"name": "Poppins",
					"slug": "poppins",
					"fontFamily": "Poppins, sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/poppins/Poppins-VariableFont_wght.woff2"
							],
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontFamily": "Inter"
						},
						{
							"src": [
								"file:./assets/fonts/poppins/Poppins-Italic-VariableFont_wght.woff2"
							],
							"fontWeight": "100 900",
							"fontStyle": "italic",
							"fontFamily": "Inter"
						}
					]
				},
				{
					"name": "Quicksand",
					"slug": "quicksand",
					"fontFamily": "Quicksand, sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/quicksand/Quicksand-VariableFont_wght.woff2"
							],
							"fontWeight": "300 700",
							"fontStyle": "normal",
							"fontFamily": "Inter"
						}
					]
				},
				{
					"name": "Roboto",
					"slug": "roboto",
					"fontFamily": "Roboto, sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/roboto/Roboto-VariableFont_wdth_wght.woff"
							],
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontFamily": "Inter"
						},
						{
							"src": [
								"file:./assets/fonts/roboto/Roboto-Italic-VariableFont_wdth_wght.woff"
							],
							"fontWeight": "100 900",
							"fontStyle": "italic",
							"fontFamily": "Inter"
						}
					]
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"typography": {
			"fontFamily": "var:preset|font-family|inter",
			"fontSize": "var:preset|font-size|small",
			"fontWeight": "300",
			"letterSpacing": "-0.1px",
			"lineHeight": "1.4"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var:preset|color|light-gray",
					"text": "var:preset|color|contrast"
				},
				"css": "font-weight: 600;transition: all 0.1s ease-in",
				"spacing": {
					"padding": {
						"top": "8px",
						"right": "25px",
						"bottom": "8px",
						"left": "25px"
					}
				},
				"border": {
					"color": "var:preset|color|light-gray",
					"width": "2px",
					"style": "solid",
					"radius": "5px"
				},
				"typography": {
					"fontSize": "var:preset|font-size|small"
				},
				":hover": {
					"color": {
						"background": "var:preset|color|dark-gray",
						"text": "var:preset|color|base"
					},
					"border": {
						"color": "var:preset|color|dark-gray"
					}
				}
			},
			"input": {
				"color": {
					"background": "var:preset|color|light-gray"
				}
			},
			"caption": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"lineHeight": "1.4"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|xx-large"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|x-large"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|large"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|medium"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"letterSpacing": "0.5px"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"fontWeight": "700",
					"letterSpacing": "1.4px",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "400",
					"lineHeight": "1.125",
					"letterSpacing": "-0.1px"
				}
			},
			"link": {
				"color": {
					"text": "var:preset|color|dark-blue"
				},
				":hover": {
					"color": {
						"text": "var:preset|color|blue"
					}
				}
			}
		},
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"css": ".wp-block-button__link:hover{background: var(--wp--preset--color--light-gray)}",
						"spacing": {
							"padding": {
								"top": "8px",
								"right": "25px",
								"bottom": "8px",
								"left": "25px"
							}
						},
						"border": {
							"color": "var:preset|color|light-gray",
							"width": "2px",
							"style": "solid",
							"radius": "5px"
						}
					},
					"blue": {
						"color": {
							"background": "var:preset|color|blue",
							"text": "var:preset|color|base"
						},
						"css": ".wp-block-button__link:hover{background: var(--wp--preset--color--dark-blue);border-color: var(--wp--preset--color--dark-blue)}",
						"spacing": {
							"padding": {
								"top": "8px",
								"right": "25px",
								"bottom": "8px",
								"left": "25px"
							}
						},
						"border": {
							"color": "var:preset|color|blue",
							"width": "2px",
							"style": "solid",
							"radius": "5px"
						}
					},
					"orange": {
						"color": {
							"background": "var:preset|color|orange",
							"text": "var:preset|color|base"
						},
						"css": ".wp-block-button__link:hover{background: var(--wp--preset--color--dark-orange);border-color: var(--wp--preset--color--dark-orange)}",
						"spacing": {
							"padding": {
								"top": "8px",
								"right": "25px",
								"bottom": "8px",
								"left": "25px"
							}
						},
						"border": {
							"color": "var:preset|color|orange",
							"width": "2px",
							"style": "solid",
							"radius": "5px"
						}
					}
				}
			},
			"core/navigation": {
				"typography": {},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var:preset|color|orange"
							}
						},
						"typography": {
							"fontWeight": "500",
							"textDecoration": "none"
						}
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	]
}